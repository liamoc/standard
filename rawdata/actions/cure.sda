{
	data: {
		target: "single",
		targetAllies: true,
		targetEnemies: true,
		defaultTarget: "ally",
		damageType: "magical_cure",
		power: 20,
		battleEffect: function(source, targets) {
			var queue = new BattleQueue();
			queue.add(Event.currentBattle.damage, [this, source, targets]);
			return queue;
		}
	},
	
	compilePath: "actions/cure.sda"
}